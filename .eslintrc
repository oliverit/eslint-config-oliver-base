{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": ["error", "never"],
    "indent": ["error", "tab", {
      "SwitchCase": 1,
      "VariableDeclarator": 1,
      "outerIIFEBody": 1,
      "FunctionDeclaration": {
        "parameters": 1,
        "body": 1
      },
      "FunctionExpression": {
        "parameters": 1,
        "body": 1
      }
    }],
    "max-len": "off",
    "no-new": "off",
    "no-param-reassign": ["error", {"props":false}],
    "no-tabs": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "prefer-destructuring": "off",
    "semi": ["error", "never"]
  }
}
